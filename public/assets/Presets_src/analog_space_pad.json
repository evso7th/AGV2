{
  "name": "Analog Space Pad",
  "description": "A warm, evolving pad with a soaring, airy top layer and subtle, detuned analog character.",
  "effects": {
    "delay": { "wet": 0.0, "time": 0.375, "feedback": 0.6 },
    "chorus": { "depth": 0.5, "rate": 0.25, "wet": 0.4 },
    "flanger": { "wet": 0.0, "rate": 0.1, "depth": 0.004, "feedback": 0.7 }
  },
  "layers": [
    {
      "id": "main-body",
      "label": "Body",
      "oscillator": { "type": "fatsawtooth", "count": 6, "spread": 30 },
      "envelope": { "attack": 0.8, "decay": 1.2, "sustain": 0.7, "release": 3.0 },
      "distortion": { "amount": 0.0 },
      "filter": { "type": "lowpass", "cutoff": 1800, "resonance": 0.4, "envMod": 500 },
      "mix": 0.8,
      "pan": 0,
      "detune": -2,
      "octave": 0,
      "delaySend": 0.0,
      "flangerSend": 0.0,
      "lfo": { "target": "pitch", "type": "triangle", "frequency": 0.2, "depth": 0.03 }
    },
    {
      "id": "air-layer",
      "label": "Air",
      "oscillator": { "type": "triangle" },
      "envelope": { "attack": 0.1, "decay": 0.5, "sustain": 0.3, "release": 1.5 },
      "distortion": { "amount": 0.0 },
      "filter": { "type": "highpass", "cutoff": 1200, "resonance": 0.1, "envMod": 0 },
      "mix": 0.4,
      "pan": 0.4,
      "detune": 0,
      "octave": 1,
      "delaySend": 0.0,
      "flangerSend": 0.0,
      "lfo": { "target": "cutoff", "type": "triangle", "frequency": 0.8, "depth": 400 }
    },
    {
      "id": "sub-bass",
      "label": "Sub",
      "oscillator": { "type": "sine" },
      "envelope": { "attack": 0.01, "decay": 0.1, "sustain": 0.9, "release": 0.5 },
      "distortion": { "amount": 0.0 },
      "filter": { "type": "lowpass", "cutoff": 150, "resonance": 1, "envMod": 0 },
      "mix": 0.9,
      "pan": 0,
      "detune": 0,
      "octave": -1,
      "delaySend": 0.0,
      "flangerSend": 0.0,
      "lfo": { "target": "pitch", "type": "sine", "frequency": 0, "depth": 0 }
    }
  ]
}