
{
  "name": "String Section",
  "description": "A preset designed to emulate the sound of a string section, with a focus on violin characteristics.",
  "portamento": 0.02,
  "layers": [
    {
      "id": "violin-body",
      "label": "Violin Body",
      "oscillator": {
        "type": "fatsawtooth",
        "count": 3,
        "spread": 8
      },
      "envelope": {
        "attack": 0.05,
        "decay": 0.4,
        "sustain": 0.8,
        "release": 0.3
      },
      "filter": {
