        "cutoff": 3500,
        "resonance": 0.4,
        "envMod": 0.3
      },
      "lfo": {
        "type": "sine",
        "frequency": 5.5,
        "depth": 0.005,
        "target": "pitch"
      },
      "pan": 0,
      "mix": 1.0,
      "octave": 0
    }
  ],
  "effects": {
    "reverb": { "decay": 0.1, "preDelay": 0.0 },
    "delay": { "wet": 0.0, "time": 0.00, "feedback": 0.0 },
    "chorus": { "depth": 0.2, "rate": 0.2, "wet": 0.035 },
    "flanger": { "wet": 0.05, "rate": 0.05, "depth": 0.003, "feedback": 0.055 }
   }
